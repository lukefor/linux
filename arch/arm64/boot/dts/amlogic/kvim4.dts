&cpu0 {
	cpu-supply = <&vddcpu>;
	/*set multi table cpufreq max*/
	multi_tables_available;
	operating-points-v2 = <&cpu_opp_table0>,
			<&cpu_opp_table1>,
			<&cpu_opp_table2>,
			<&cpu_opp_table3>;
	clocks = <&clkc CLKID_CPU_CLK>,
	       <&clkc CLKID_CPU_CLK_DYN>,
	       <&clkc CLKID_SYS_PLL>,
	       <&clkc CLKID_DSU_CLK>,
	       <&clkc CLKID_DSU_CLK_FINAL>;
	clock-names = "core_clk",
		"low_freq_clk_parent",
		"high_freq_clk_parent",
		"dsu_clk",
		"dsu_pre_parent";
	clock-latency = <50000>;
};

